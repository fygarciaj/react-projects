generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model Cierre {
  Id                                 Int                          @id(map: "PK_Adm_Cierres") @default(autoincrement())
  IdCaja                             Int
  FechaInicial                       DateTime
  FechaFinal                         DateTime?
  IdTercero                          Int
  IdIdentificador                    Int
  Identificador                      String                       @db.VarChar(50)
  PumpCierre                         Int?
  PumpIdVentaInicial                 Int?
  PumpIdVentaFinal                   Int?
  GranTotalDineroSumatoriaBruto      Decimal                      @default(0, map: "DF_Adm_Cierres_GranTotalDineroSumatoriaBruto") @db.Decimal(18, 3)
  GranTotalDineroTotalizadorBruto    Decimal                      @default(0, map: "DF_Adm_Cierres_GranTotalDineroTotalizadorBruto") @db.Decimal(18, 3)
  GranTotalDineroTotalizadorNeto     Decimal                      @default(0, map: "DF_Adm_Cierres_GranTotalDineroTotalizadorNeto") @db.Decimal(18, 3)
  IdLocal                            Int?
  SegIdSync                          BigInt?
  SegUsuarioBdCreacion               String?                      @db.VarChar(100)
  SegUsuarioBdModificacion           String?                      @db.VarChar(100)
  SegUsuarioWebCreacion              String?                      @db.VarChar(256)
  SegUsuarioWebModificacion          String?                      @db.VarChar(256)
  SegFechaCreacion                   DateTime?
  SegFechaModificacion               DateTime?
  SegHostIp                          String?                      @db.VarChar(Max)
  IdTurnoVendedor                    Int                          @default(0, map: "DF_Adm_Cierres_IdTurnoVendedor")
  FechaExportado                     DateTime?
  Observaciones                      String?                      @db.VarChar(Max)
  IdTurno                            Int                          @default(0, map: "DF_Adm_Cierres_IdTurno")
  GranTotalDineroTotalizadorMecanico Decimal                      @default(0, map: "DF_Adm_Cierres_GranTotalDineroTotalizadorMecanico") @db.Decimal(18, 3)
  Gen_Turnos                         Gen_Turnos                   @relation(fields: [IdTurno], references: [Id], onUpdate: NoAction, map: "FK_Adm_Cierres_Gen_Turnos")
  Par_Cajas                          Par_Cajas                    @relation(fields: [IdCaja], references: [Id], onUpdate: NoAction, map: "FK_Adm_Cierres_Par_Cajas")
  Rrhh_TurnosDeVendedores            Rrhh_TurnosDeVendedores      @relation(fields: [IdTurnoVendedor], references: [Id], onUpdate: NoAction, map: "FK_Adm_Cierres_Rrhh_TurnosDeVendedores")
  Ter_Identificadores                Ter_Identificadores          @relation(fields: [IdIdentificador], references: [Id], onUpdate: NoAction, map: "FK_Adm_Cierres_Ter_Identificadores")
  Ter_Terceros                       Ter_Terceros                 @relation(fields: [IdTercero], references: [Id], onUpdate: NoAction, map: "FK_Adm_Cierres_Ter_Terceros")
  Adm_CierresPorFlotas               Adm_CierresPorFlotas[]
  Adm_CierresPorFormaDePagos         Adm_CierresPorFormaDePagos[]
  Adm_CierresPorMangueras            Adm_CierresPorMangueras[]
  Adm_CierresPorProductos            Adm_CierresPorProductos[]
  Adm_DepositosPorCierres            Adm_DepositosPorCierres[]
  Adm_Ventas                         Adm_Ventas[]
  Men_Mensajes                       Men_Mensajes[]

  @@index([IdCaja, IdLocal], map: "Adm_Cierres_IdCaja_IdLocal")
  @@index([FechaInicial, FechaFinal], map: "IX_Adm_Cierres_FechaInicial_FechaFinal")
  @@index([IdLocal(sort: Desc)], map: "IX_Adm_Cierres_IdLocal")
  @@index([IdIdentificador], map: "IX_AdmCierres_IdIdentificador")
  @@map("Adm_Cierres")
}

